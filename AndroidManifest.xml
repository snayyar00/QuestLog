<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Workshop">
        <activity android:name=".VideoSelectActivity"
            android:parentActivityName=".AssignmentsActivity"></activity>
        <activity
            android:name=".AssignmentsActivity"
            android:parentActivityName=".CoursesActivity" />
        <activity android:name=".VideoActivity" />
        <activity
            android:name=".SettingsActivity"
            android:parentActivityName=".SubjectsActivity" />
        <activity
            android:name=".CoursesActivity"
            android:parentActivityName=".SubjectsActivity" />
        <activity android:name=".SubjectsActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
